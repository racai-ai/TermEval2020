#!/data/vasile/term_extractor/venv_yake/bin/python3.7
# EASY-INSTALL-ENTRY-SCRIPT: 'segtok==1.5.7','console_scripts','segmenter'
__requires__ = 'segtok==1.5.7'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('segtok==1.5.7', 'console_scripts', 'segmenter')()
    )
